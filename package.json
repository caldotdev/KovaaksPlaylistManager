{
  "name": "kovaaks-playlist-manager",
  "version": "0.0.1",
  "description": "Tool to manage playlist files in Kovaaks FPS Aim Trainer",
  "main": "./src/electron.js",
  "license": "MIT",
  "author": {
    "name": "sens",
    "email": "coluckenbac@adct.com",
    "url": "https://twitter.com/sens0001"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sens0001/KovaaksPlaylistManager"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "electron-forge start",
    "electron": "run-s build pure-electron",
    "pure-electron": "electron .",
    "electron-dev": "run-p dev pure-electron-dev",
    "pure-electron-dev": "cross-env NODE_ENV=development electron .",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "autoprefixer": "^10.1.0",
    "chokidar": "^3.4.3",
    "cross-env": "^7.0.3",
    "electron": "^11.1.0",
    "eslint": "^7.16.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-svelte3": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.1",
    "postcss-nesting": "^7.0.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^1.0.10",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.6.1",
    "tailwindcss": "^2.0.2"
  },
  "dependencies": {
    "electron-is-dev": "^1.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^6.0.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "kovaaks_playlist_manager",
            "exe": "KovaaksPlaylistManager",
            "certificateFile": "./cert.pfx",
            "certificatePassword": "this-is-a-secret"
          }
        }
      ]
    }
  }
}
